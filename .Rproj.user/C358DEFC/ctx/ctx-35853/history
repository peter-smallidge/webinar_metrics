input <- read_excel(file_path_zoom, sheet = "full_cleaned", skip=10) %>%
mutate(acres = str_remove_all(acres, "[,\\+ ]")) %>% #removes those characters from the acres varaible
mutate(acres = if_else(
str_detect(acres, "-"),
str_extract(acres, "(?<=-)[0-9]+"),  # Extract the number after the hyphen
acres
)) %>%
mutate(join = str_remove(join, "\\b\\d{1,2}/\\d{1,2}/\\d{4}\\b")) %>%
mutate(leave = str_remove(leave, "\\b\\d{1,2}/\\d{1,2}/\\d{4}\\b"))
